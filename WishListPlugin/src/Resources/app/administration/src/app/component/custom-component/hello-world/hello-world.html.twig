        v-bind="$attrs"
        :value="defaultValue"
        :type="fieldType"
        :disabled="isDisabled"
        @change="setInput"
    />


    <div class="sw-field--switch sw-form-field-renderer sw-field--switch-bordered is--bordered sw-field--default __web-inspector-hide-shortcut__">
