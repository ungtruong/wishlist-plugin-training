            <div class="sw-field--switch__input">
                <input id="check" type="checkbox" name="WishlistPlugin.config.isMultive" :checked="isChecked" @change="checkInput2" />
                <div class="sw-field__switch-state">
                    <div class="sw-field__switch-state-knob"></div>
                </div>
            </div>
            <div class="sw-field">
                <div class="sw-field__label">
                    <!---->
                    <label for="check" class="">
                        Allow multiple wishlists
                    </label>
                    <!---->
                </div>
                <!---->
                <!---->
            </div>
        </div>
        <!---->
    </div>

</div>






